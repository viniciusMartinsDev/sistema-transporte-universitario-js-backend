{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "airbnb-base",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-tabs": "off",
		"no-template-curly-in-string": "off",
		"indent": [
			"error",
			"tab"
		],
		"semi": [
			"error",
			"never"
		],
		"no-console": "off"
	}
}